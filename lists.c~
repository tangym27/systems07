#include <stdio.h>
#include <stdlib.h>

struct node { 
	int i; 
	struct node *next; 
};

int SIZE = sizeof(struct node);
struct node head;



void print_list(struct node *n){
	int i = 0;
	while (n != NULL){
		printf("Node %d: %d -> %p\n", i, n->i, n->next);
		n = n->next;
		i++;
	}
}

struct node * insert_front(struct node *n, int i){
	struct node *s = malloc(SIZE);
	s->next = n;
	s->i = i;
	return s;
}

struct node * free_list(struct node *n){
	struct node *head = n;
	while (n != NULL){
		struct node *holder = n->next;
		free(n);
		n = holder;
	}
	return head;
}


int main(){
	insert_front(head, 5);
	return 0;
}

